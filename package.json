{
  "name": "@sippy-platform/valkyrie",
  "version": "1.0.0-alpha.9",
  "description": "The iconography of Sippy.",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "fonts": "fantasticon",
    "icons": "npm run icons:clean && npm run icons:ts",
    "icons:clean": "svgo -f ./icons -o ./icons",
    "icons:ts": "svg-to-ts-constants",
    "test": "echo \"Error: no test specified\" && exit 1",
    "pages": "node build/generate-pages.js --verbose",
    "prod": "npm run icons && npm run fonts && npm run build && npm run scss",
    "scss": "npm run scss:compile && npm run scss:prefix",
    "scss:compile": "sass font/Valkyrie.scss font/Valkyrie.css --no-source-map",
    "scss:prefix": "postcss -u autoprefixer -r ./font/*.css --map"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sippy-platform/valkyrie.git"
  },
  "files": [
    "/icons",
    "/font",
    "/dist"
  ],
  "keywords": [
    "icons",
    "svg-icons",
    "iconography"
  ],
  "author": "",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/sippy-platform/valkyrie/issues"
  },
  "homepage": "https://sippy-platform.github.io/valkyrie",
  "peerDependencies": {
    "@emotion/react": ">=11.x",
    "@emotion/styled": ">=11.x",
    "@mui/system": ">=5.x",
    "react": ">=16.x"
  },
  "devDependencies": {
    "@types/react": "18.0.27",
    "autoprefixer": "10.4.13",
    "bundle-scss": "1.5.1",
    "copyfiles": "2.4.1",
    "fantasticon": "1.2.3",
    "nodemon": "2.0.20",
    "npm-run-all": "4.1.5",
    "picocolors": "1.0.0",
    "postcss": "8.4.21",
    "postcss-cli": "10.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "1.58.0",
    "svg-to-ts": "9.0.0",
    "svgo": "3.0.2",
    "typescript": "4.9.5"
  },
  "browserslist": {
    "production": [
      "last 2 versions",
      "not dead",
      "not ie <= 11"
    ]
  },
  "sideEffects": false
}
