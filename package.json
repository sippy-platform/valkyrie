{
  "name": "@optimisegroup/valkyrie",
  "version": "0.14.1",
  "description": "The iconography of Sippy.",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "clean": "rimraf dist",
    "docs:fonts": "copyfiles -a \"dist/fonts/*.*\" \"docs/fonts\" -f",
    "fonts": "npm run fonts:generate && npm run fonts:docs",
    "fonts:generate": "mkdirp dist/fonts && fantasticon",
    "fonts:docs": "copyfiles -a \"dist/fonts/*.*\" \"docs/fonts\" -f",
    "icons": "npm run icons:clean && npm run icons:ts",
    "icons:clean": "svgo -f ./icons -o ./icons",
    "icons:ts": "svg-to-ts-constants",
    "mellow": "svgo -f ./mellow -o ./mellow",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "npm run prod",
    "prod": "npm run clean && npm run icons && npm run fonts && npm run build && npm run scss",
    "scss": "npm run scss:compile && npm run scss:prefix && npm run scss:copy",
    "scss:compile": "node-sass src -o dist --output-style compressed",
    "scss:copy": "bundle-scss --mask \"src/valkyrie.scss\" --dest \"dist/valkyrie.scss\"",
    "scss:prefix": "postcss -u autoprefixer -r dist/*.css --map"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/optimise-group/valkyrie.git"
  },
  "files": [
    "/icons",
    "/dist"
  ],
  "keywords": [
    "icons",
    "svg-icons",
    "iconography"
  ],
  "author": "",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/optimise-group/valkyrie/issues"
  },
  "homepage": "https://optimise-group.github.io/valkyrie",
  "peerDependencies": {
    "react": ">=16.x",
    "clsx": ">=1.x"
  },
  "devDependencies": {
    "@types/react": "17.0.43",
    "autoprefixer": "10.4.4",
    "bundle-scss": "1.5.1",
    "clsx": "1.1.1",
    "copyfiles": "2.4.1",
    "fantasticon": "1.2.2",
    "node-sass": "7.0.1",
    "nodemon": "2.0.15",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.12",
    "postcss-cli": "9.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rimraf": "3.0.2",
    "sass": "1.49.9",
    "svg-to-ts": "8.4.0",
    "svgo": "2.8.0",
    "typescript": "4.6.3"
  },
  "browserslist": {
    "production": [
      "last 2 versions",
      "not dead",
      "not ie <= 11"
    ]
  },
  "sideEffects": false
}
